{
  "name": "story-vault-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "debug": "concurrently \"google-chrome --remote-debugging-port=9222 --user-data-dir=/tmp/chrome-debug http://localhost:5173\" \"vite\"",
    "dev": "vite",
    "start": "vite",
    "start-secure": "cross-env VITE_USE_HTTPS=true vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "analyze": "vite build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@mantine/core": "^8.2.4",
    "@mantine/form": "^8.2.4",
    "@mantine/hooks": "^8.2.4",
    "@mantine/notifications": "^8.2.4",
    "@tanstack/react-query": "^5.81.5",
    "@types/styled-components": "^5.1.34",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.0",
    "styled-components": "^6.1.19",
    "uuid": "^11.1.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^23.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^1.6.1"
  }
}
